$group
	
	e_tag( variable, ref
		a type
		r comment/s
		r directive/s

	e_tag( stringVar, string, variable
		m wzInitialize
			set this.type = 'String'

	e_tag( intVar, int, variable
		m wzInitialize
			set this.type = 'Int'

	e_tag( floatVar, float, variable
		m wzInitialize
			set this.type = 'Float'

	e_tag( booleanVar, boolean, variable
		m wzInitialize
			set this.type = 'Boolean'
	


