$group

	var filePathDelete = "c:/my/starters/darvin/hello.md"
	var content = "Hello world"

	describe delete a file

		before-async
			_ doc.writeFile
				@ filePathDelete
				@ content
				f_cb_done( r )

		it-async should delete a file
			_ doc.deleteFile
				@ filePathDelete
				f_cb_done( r )
					log 'should delete a file', err, r
					tobe_true( r.deleted )
					tobe_number( r.n, 1 )
					tobe_number( r.ok, 1 )


