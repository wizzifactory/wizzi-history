module
	kind jsfile

	import path
	import del
	var vfile = require('../../lib/fs/vfile')
	var file = vfile()

	_ del.sync
		_ path.join
			@ __dirname
			@ 'ittf'
			@ '**'
			@ '*.*'

	_ file.write
		_ path.join
			@ __dirname
			@ 'ittf'
			@ 't'
			@ 'alpha.tests.ittf'
		@ 'tests makepath'
		f_cb_throw( result )
			log 'result', result

