artifact( json, export, noschemacheck )

	$append imports
		var stringify = require('json-stringify-safe')
    
	$append main
		var json
		if model.toJson
			set json
				_ model.toJson()
		else
			return
				_ callback
					_ error
						@ 'The model has no toJson method'
		_ ctx.w
			_ stringify(json, null, 4)