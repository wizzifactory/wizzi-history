$group

	{
		&&
			+ onCreateFolder
			< Tooltip
				@ title Create new folder
				< Button
					@ className {classes.navLinkButton}
					@ onClick {onCreateFolder}
					< CreateNewFolderIcon
	{
		&&
			+ onCreateFile
			< Tooltip
				@ title Create new file
				< Button
					@ className {classes.navLinkButton}
					@ onClick {onCreateFile}
					< CreateFileIcon