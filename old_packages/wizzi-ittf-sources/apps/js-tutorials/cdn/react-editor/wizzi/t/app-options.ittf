$group

	$include app/options/init

	$
		app.name = 'cdn-react-editor';
		app.description = 'Wizzi factory demos - cdn + react + editor';
	
		app.useReact = true;
		app.useMaterialUiIcons = true;
		app.useSplitter = true;
		app.useWizziUtils = true;

		app.isLibrary = true;
		app.libraryName = 'reactEditorLib';

	$include app/options/analize