$group

	get( /api/wf/load )

		check-params( )

			not-empty( query, uri)

		var sessionData = {}
		var modelContext = {}
		var artifactContext = {}
		
		_ wfApi.loadModel
			@ sessionData
			@ uri
			@ modelContext
			f_cb_rest( result )
				_ sendSuccess
					@ res
					@ result.status
					@ result.data


