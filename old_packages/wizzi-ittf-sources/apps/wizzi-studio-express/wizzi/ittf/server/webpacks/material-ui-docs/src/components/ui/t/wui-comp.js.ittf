module
	$params name
	kind react
	+
	import React from 'react'
	import PropTypes from 'prop-types'
	import classNames from 'classnames'
	import { createStyleSheet } from 'jss-theme-reactor'
	import withStyles from '../styles/withStyles'
	+
	$hook import
	+
	export const styleSheet =
		_ createStyleSheet
			@ 'Wui${name}'
			=>
				param theme
				$hook styles-before
				return
					{
						$hook styles
	+
	class ${name}
