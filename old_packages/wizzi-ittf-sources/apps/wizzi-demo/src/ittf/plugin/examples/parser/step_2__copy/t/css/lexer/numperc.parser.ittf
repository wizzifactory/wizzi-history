$group
	$params kind|main

	$if kind === 'main'
		whitespace( sub )

	$if kind === 'main' || kind === 'sub'
		e num
			|
				+
					[ 0-9
				&
					*
						[ 0-9
					: .
					+
						[ 0-9

	$if kind === 'main' || kind === 'tokens'
		tk Percentage
			pattern
				&
					(
						ref num
					: %

		tk Number
			pattern
				(
					ref num

	$if kind === 'main'
		test 234
		test .23
		test 10.05
		test .50%
		test 10.2%
		test 33%
