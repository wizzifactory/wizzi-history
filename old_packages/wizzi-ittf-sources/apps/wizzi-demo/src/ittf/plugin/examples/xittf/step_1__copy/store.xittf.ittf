xittf first

	function-provider scss

	@var white = '#fff';
	@var green
		: '#00ff32'

	@function sum
		param a
		param b
		return a + b
		
	@mixin alert-class
		@param color
		. alert
			color
				@_ darken
					: color
					_ sum
						: 2
						: 3
			background-color
				@: white
	@mix alert-class
		@: green