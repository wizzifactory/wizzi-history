            type anyURI
            is-optional
    ag animAttributeAttrs
        a attributeName
            type string
            is-required
        a attributeType
            type string
            is-optional
    ag animTargetAttrs
        ag-ref svg:animElementAttrs
        ag-ref svg:animAttributeAttrs
    ag animTimingAttrs
        a begin
            type string
            is-optional
        a dur
            type string
            is-optional
        a end
            type string
            is-optional
        a min
            type string
            is-optional
        a max
            type string
            is-optional
        a restart
