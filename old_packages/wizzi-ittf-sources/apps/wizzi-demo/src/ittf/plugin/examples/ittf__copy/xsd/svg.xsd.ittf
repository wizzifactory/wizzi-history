            s-type 
                restrict : string
                    enum always
                    enum never
                    enum whenNotActive
        a repeatCount
            type string
            is-optional
        a repeatDur
            type string
            is-optional
        a fill
            default remove
            s-type 
                restrict : string
                    enum remove
                    enum freeze
    ag animValueAttrs
        a calcMode
            default linear
            s-type 
                restrict : string
                    enum discrete
                    enum linear
                    enum paced
                    enum spline
        a values
            type string
            is-optional
        a keyTimes
            type string
            is-optional
        a keySplines
            type string
            is-optional
        a from
            type string
            is-optional
        a to
            type string
            is-optional
        a by
            type string
            is-optional
    ag animAdditionAttrs
        a additive
            default replace
            s-type 
                restrict : string
                    enum replace
                    enum sum
        a accumulate
            default none
            s-type 
                restrict : string
                    enum none
                    enum sum
    group descTitleMetadata
        # A bit simpler than the DTD, but see commented-out alternative
        all 
            e desc
                type svg:descType
                min-occurs 0
            e title
                type svg:titleType
                min-occurs 0
            e metadata
                type svg:metadataType
                min-occurs 0
