xsd 
    target-namespace http://www.w3.org/2000/svg
    xmlns http://www.w3.org/2001/XMLSchema
    xmlns svg http://www.w3.org/2000/svg
    xmlns xlink http://www.w3.org/1999/xlink
    element-form-default unqualified
    attribute-form-default unqualified
    language en
    import xlink.xsd
        namespace http://www.w3.org/1999/xlink
    import xml.xsd
        namespace http://www.w3.org/XML/1998/namespace
    s-type BaselineShiftValueType
        # 
            # The actual definition is
            # baseline | sub | super | <percentage> | <length> | inherit
            # not sure that union can do this
            # 
        restrict : string
    s-type ClassListType
        # Space-separated list of classes
        list string
    s-type ClipValueType
        # <shape> | auto | inherit
        restrict : string
    s-type ClipPathValueType
        # <uri> | none | inherit
        restrict : string
    s-type ClipFillRuleType
        # 'clip-rule' or fill-rule property/attribute value
        restrict : string
            enum evenodd
            enum nonzero
            enum inherit
    s-type ContentTypeType
        # 
            # source http://www.ietf.org/rfc/rfc2045.txt
        # media type, as per [RFC2045]
        restrict : string
    s-type CoordinateType
        # 
            # source http://www.w3.org/TR/SVG/types.html#DataTypeCoordinate
        # a coordinate, which is a number optionally followed immediately by a unit identifier. Perhaps it is possible to represent this as a union by declaring unit idenifiers as a type?
        restrict : string
            pattern ((((\+|\-)?((\d+)))|((\+|\-)?(((((\d+)?\.(\d+))|((\d+)\.))([eE](\+|\-)?(\d+))?)|((\d+)([eE](\+|\-)?(\d+))))))(em|ex|px|pt|pc|cm|mm|in|%)?)
    s-type CoordinatesType
        # a space separated list of CoordinateType. Punt to 'string' for now
        restrict : string
    s-type ColorType
        # 
            # source http://www.w3.org/TR/SVG/types.html#DataTypeColor
        # Color as defined in CSS2 and XSL 1.0 plus additional recognised color keyword names (the 'X11 colors')
        restrict : string
    s-type CursorValueType
        # 
            # Value is an optional comma-separated list orf uri references followed by one token from an enumerated list.
            # 
        # [ [<uri> ,]* [ auto | crosshair | default | pointer | move | e-resize | ne-resize | nw-resize | n-resize | se-resize | sw-resize | s-resize | w-resize| text | wait | help ] ] | inherit
        restrict : string
    s-type EnableBackgroundValueType
        # accumulate | new [ <x> <y> <width> <height> ] | inherit
        restrict : string
    s-type ExtensionListType
        # extension list specification
        restrict : string
    s-type FeatureListType
        # feature list specification
        restrict : string
    s-type FilterValueType
        # 
            # <uri> | none | inherit
            # 
        restrict : string
    s-type FontFamilyValueType
        # [[ <family-name> | <generic-family> ],]* [<family-name> | <generic-family>] | inherit
        # 'font-family' property/attribute value (i.e., list of fonts)
        restrict : string
    s-type FontSizeValueType
        # 'font-size' property/attribute value
        # <absolute-size> | <relative-size> | <length> | <percentage> | inherit
        restrict : string
    s-type FontSizeAdjustValueType
        # 'font-size-adjust' property/attribute value
        # <number> | none | inherit
        restrict : string
    s-type GlyphOrientationHorizontalValueType
        # 'glyph-orientation-horizontal' property/attribute value (e.g., <angle>)
        # <angle> | inherit
        restrict : string
    s-type GlyphOrientationVerticalValueType
        # 'glyph-orientation-vertical' property/attribute value (e.g., 'auto', <angle>)
        # auto | <angle> | inherit
        restrict : string
    s-type KerningValue
        # 'kerning' property/attribute value (e.g., auto | <length>)
        # auto | <length> | inherit
        restrict : string
    s-type LanguageCodeType
        # a language code, as per [RFC3066]
        # 
            # source http://www.ietf.org/rfc/rfc3066.txt
        restrict : string
    s-type LanguageCodesType
        # a comma-separated list of language codes, as per [RFC3066]
        # 
            # source http://www.ietf.org/rfc/rfc3066.txt
        restrict : string
    s-type LengthType
        # a <length>
        restrict : string
            pattern ((((\+|\-)?((\d+)))|((\+|\-)?(((((\d+)?\.(\d+))|((\d+)\.))([eE](\+|\-)?(\d+))?)|((\d+)([eE](\+|\-)?(\d+))))))(em|ex|px|pt|pc|cm|mm|in|%)?)
    s-type LengthsType
        # a list of <length>s
        restrict : string
    s-type LinkTargetType
        # link to this target
        restrict : string
    s-type MarkerValueType
        # 'marker' property/attribute value (e.g., 'none', %URI;)
        restrict : string
    s-type MaskValueType
        # 'mask' property/attribute value (e.g., 'none', %URI;)
        # <uri> | none | inherit
        restrict : string
    s-type MediaDescType
        # comma-separated list of media descriptors.
        restrict : string
    s-type NumberOptionalNumberType
        # list of <number>s, but at least one and at most two
        restrict : string
    s-type NumberOrPercentageType
        # a <number> or a  <percentage>
        restrict : string
    s-type NumbersType
        # list of <number>s
        restrict : string
    s-type OpacityValueType
        # opacity value (e.g., <number>)
        # <alphavalue> | inherit
        restrict : string
    s-type PaintType
        # a 'fill' or 'stroke' property/attribute value
        restrict : string
    s-type PathDataType
        # a path data specification
        # 
            # source http://www.w3.org/TR/SVG/paths.html
        # Yes, of course this was generated by a program!
        restrict : string
    s-type PointsType
        # a list of points
        restrict : string
    s-type PreserveAspectRatioSpecType
        # 'preserveAspectRatio' attribute specification
        restrict : string
            pattern (\s*("none"|x("Min"|"Mid"|"Max")y("Min"|"Mid"|"Max"))\s+("meet"|"slice")?\s*)
    s-type ScriptType
        # script expression
        restrict : string
    s-type SpacingValueType
        # 'letter-spacing' or 'word-spacing' property/attribute value (e.g., normal | <length>)
        restrict : string
    s-type StrokeDashArrayValueType
        # 'stroke-dasharray' property/attribute value (e.g., 'none', list of <number>s)
        restrict : string
    s-type StrokeDashOffsetValueType
        # 'stroke-dashoffset' property/attribute value (e.g., 'none', >length>)
        restrict : string
    s-type StrokeMiterLimitValueType
        # 'stroke-miterlimit' property/attribute value (e.g., <number>)
        restrict : string
    s-type StrokeWidthValueType
        # 'stroke-width' property/attribute value (e.g., <length>)
        restrict : string
    s-type StyleSheetType
        # style sheet data
        restrict : string
    s-type SVGColorType
        # An SVG color value (sRGB plus optional ICC)
        restrict : string
    s-type TextDecorationValueType
        # 'text-decoration' property/attribute value (e.g., 'none', 'underline')
        restrict : string
    s-type TransformListType
        # Yes, of course this was generated by a program!
        # list of transforms
        restrict : string
    s-type ViewBoxSpecType
        # 'viewBox' attribute specification
        restrict : string
    ag stdAttrs
        # All elements have an ID
        a id
            type ID
            is-optional
        a-ref xml:base
            type anyURI
            is-optional
    ag langSpaceAttrs
        # Common attributes for elements that might contain character data content
        a-ref xml:lang
            is-optional
        a-ref xml:space
            is-optional
    ag testAttrs
        # Common attributes to check for system capabilities
        a requiredFeatures
            type svg:FeatureListType
            is-optional
        a requiredExtensions
            type svg:ExtensionListType
            is-optional
        a systemLanguage
            type svg:LanguageCodesType
            is-optional
    ag xlinkRefAttrs
        # For most uses of URI referencing: standard XLink attributes other than xlink:href
        a-ref xlink:type
            fixed simple
        a-ref xlink:role
        a-ref xlink:arcrole
        a-ref xlink:title
        a-ref xlink:show
            default other
        a-ref xlink:actuate
    ag xlinkRefAttrsEmbed
        # Standard XLink attributes for uses of URI referencing where xlink:show is 'embed'
        a-ref xlink:type
            fixed simple
        a-ref xlink:role
        a-ref xlink:arcrole
        a-ref xlink:title
        a-ref xlink:show
        a-ref xlink:actuate
    ag graphicsElementEvents
        a onfocusin
            type svg:ScriptType
            is-optional
        a onfocusout
            type svg:ScriptType
            is-optional
        a onactivate
            type svg:ScriptType
            is-optional
        a onclick
            type svg:ScriptType
            is-optional
        a onmousedown
            type svg:ScriptType
            is-optional
        a onmouseup
            type svg:ScriptType
            is-optional
        a onmouseover
            type svg:ScriptType
            is-optional
        a onmousemove
            type svg:ScriptType
            is-optional
        a onmouseout
            type svg:ScriptType
            is-optional
        a onload
            type svg:ScriptType
            is-optional
    ag documentEvents
        a onunload
            type svg:ScriptType
            is-optional
        a onabort
            type svg:ScriptType
            is-optional
        a onerror
            type svg:ScriptType
            is-optional
        a onresize
            type svg:ScriptType
            is-optional
        a onscroll
            type svg:ScriptType
            is-optional
        a onzoom
            type svg:ScriptType
            is-optional
    ag animationEvents
        a onbegin
            type svg:ScriptType
            is-optional
        a onend
            type svg:ScriptType
            is-optional
        a onrepeat
            type svg:ScriptType
            is-optional
    ag PresentationAttributes-Color
        # The following presentation attributes have to do with specifying color.
        a color
            type svg:ColorType
            is-optional
        a color-interpolation
            is-optional
            s-type 
                restrict : string
                    enum auto
                    enum sRGB
                    enum linearRGB
                    enum inherit
        a color-rendering
            is-optional
            s-type 
                restrict : string
                    enum auto
                    enum optimizeSpeed
                    enum optimizeQuality
                    enum inherit
    ag PresentationAttributes-Containers
        # The following presentation attributes apply to container elements
        a enable-background
            type svg:EnableBackgroundValueType
            is-optional
    ag PresentationAttributes-feFlood
        # The following presentation attributes apply to 'feFlood' elements
        a flood-color
            type svg:SVGColorType
            is-optional
        a flood-opacity
            type svg:OpacityValueType
            is-optional
    ag PresentationAttributes-FilterPrimitives
        # The following presentation attributes apply to filter primitives
        a color-interpolation-filters
            is-optional
            s-type 
                restrict : string
                    enum auto
                    enum sRGB
                    enum linearRGB
                    enum inherit
    ag PresentationAttributes-FillStroke
        # The following presentation attributes apply to filling and stroking operations
        a fill
            type svg:PaintType
            is-optional
        a fill-opacity
            type svg:OpacityValueType
            is-optional
        a fill-rule
            type svg:ClipFillRuleType
            is-optional
        a stroke
            type svg:PaintType
            is-optional
        a stroke-dasharray
            type svg:StrokeDashArrayValueType
            is-optional
        a stroke-dashoffset
            type svg:StrokeDashOffsetValueType
            is-optional
        a stroke-linecap
            is-optional
            s-type 
                restrict : string
                    enum butt
                    enum round
                    enum square
                    enum inherit
        a stroke-linejoin
            is-optional
            s-type 
                restrict : string
                    enum miter
                    enum round
                    enum bevel
                    enum inherit
        a stroke-miterlimit
            type svg:StrokeMiterLimitValueType
            is-optional
        a stroke-opacity
            type svg:OpacityValueType
            is-optional
        a stroke-width
            type svg:StrokeWidthValueType
            is-optional
    ag PresentationAttributes-FontSpecification
        # The following presentation attributes have to do with selecting a font to use
        a font-family
            type svg:FontFamilyValueType
            is-optional
        a font-size
            type svg:FontSizeValueType
            is-optional
        a font-size-adjust
            type svg:FontSizeAdjustValueType
            is-optional
        a font-stretch
            is-optional
            s-type 
                restrict : string
                    enum normal
                    enum wider
                    enum narrower
                    enum ultra-condensed
                    enum extra-condensed
                    enum condensed
                    enum semi-condensed
                    enum semi-expanded
                    enum expanded
                    enum extra-expanded
                    enum ultra-expanded
                    enum inherit
        a font-style
            is-optional
            s-type 
                restrict : string
                    enum normal
                    enum italic
                    enum oblique
                    enum inherit
        a font-variant
            is-optional
            s-type 
                restrict : string
                    enum normal
                    enum small-caps
                    enum inherit
        a font-weight
            is-optional
            s-type 
                restrict : string
                    enum normal
                    enum bold
                    enum bolder
                    enum lighter
                    enum 100
                    enum 200
                    enum 300
                    enum 400
                    enum 500
                    enum 600
                    enum 700
                    enum 800
                    enum 900
                    enum inherit
    ag PresentationAttributes-Gradients
        # The following presentation attributes apply to gradient 'stop' elements
        a stop-color
            type svg:SVGColorType
            is-optional
        a stop-opacity
            type svg:OpacityValueType
            is-optional
    ag PresentationAttributes-Graphics
        # The following presentation attributes apply to graphics elements
        a clip-path
            type svg:ClipPathValueType
            is-optional
        a clip-rule
            type svg:ClipFillRuleType
            is-optional
        a cursor
            type svg:CursorValueType
            is-optional
        a display
            is-optional
            s-type 
                restrict : string
                    enum inline
                    enum block
                    enum list-item
                    enum run-in
                    enum compact
                    enum marker
                    enum table
                    enum inline-table
                    enum table-row-group
                    enum table-header-group
                    enum table-footer-group
                    enum table-row
                    enum table-column-group
                    enum table-column
                    enum table-cell
                    enum table-caption
                    enum none
                    enum inherit
        a filter
            type svg:FilterValueType
            is-optional
        a image-rendering
            is-optional
            s-type 
                restrict : string
                    enum auto
                    enum optimizeSpeed
                    enum optimizeQuality
                    enum inherit
        a mask
            type svg:MaskValueType
            is-optional
        a opacity
            type svg:OpacityValueType
            is-optional
        a pointer-events
            is-optional
            s-type 
                restrict : string
                    enum visiblePainted
                    enum visibleFill
                    enum visibleStroke
                    enum visibleFillStroke
                    enum visible
                    enum painted
                    enum fill
                    enum stroke
                    enum fillstroke
                    enum all
                    enum none
                    enum inherit
        a shape-rendering
            is-optional
            s-type 
                restrict : string
                    enum auto
                    enum optimizeSpeed
                    enum crispEdges
                    enum geometricPrecision
                    enum inherit
        a text-rendering
            is-optional
            s-type 
                restrict : string
                    enum auto
                    enum optimizeSpeed
                    enum optimizeLegibility
                    enum geometricPrecision
                    enum inherit
        a visibility
            is-optional
            s-type 
                restrict : string
                    enum visible
                    enum hidden
                    enum inherit
    ag PresentationAttributes-Images
        # The following presentation attributes apply to 'image' elements
        a color-profile
            is-optional
    ag PresentationAttributes-LightingEffects
        # The following presentation attributes apply to 'feDiffuseLighting' and 'feSpecularLighting' elements
        a lighting-color
            type svg:SVGColorType
            is-optional
    ag PresentationAttributes-Markers
        # The following presentation attributes apply to marker operations
        a marker-start
            type svg:MarkerValueType
            is-optional
        a marker-mid
            type svg:MarkerValueType
            is-optional
        a marker-end
            type svg:MarkerValueType
            is-optional
    ag PresentationAttributes-TextContentElements
        # The following presentation attributes apply to text content elements
        a alignment-baseline
            is-optional
            s-type 
                restrict : string
                    enum baseline
                    enum top
                    enum before-edge
                    enum text-top
                    enum text-before-edge
                    enum middle
                    enum bottom
                    enum after-edge
                    enum text-bottom
                    enum text-after-edge
                    enum ideographic
                    enum lower
                    enum hanging
                    enum mathematical
                    enum inherit
        a baseline-shift
            type svg:BaselineShiftValueType
            is-optional
        a direction
            is-optional
            s-type 
                restrict : string
                    enum ltr
                    enum rtl
                    enum inherit
        a dominant-baseline
            is-optional
            s-type 
                restrict : string
                    enum auto
                    enum autosense-script
                    enum no-change
                    enum reset
                    enum ideographic
                    enum lower
                    enum hanging
                    enum mathematical
                    enum inherit
        a glyph-orientation-horizontal
            type svg:GlyphOrientationHorizontalValueType
            is-optional
        a glyph-orientation-vertical
            type svg:GlyphOrientationVerticalValueType
            is-optional
        a letter-spacing
            type svg:SpacingValueType
            is-optional
        a text-anchor
            is-optional
            s-type 
                restrict : string
                    enum start
                    enum middle
                    enum end
                    enum inherit
        a text-decoration
            type svg:TextDecorationValueType
            is-optional
        a unicode-bidi
            is-optional
            s-type 
                restrict : string
                    enum normal
                    enum embed
                    enum bidi-override
                    enum inherit
        a word-spacing
            type svg:SpacingValueType
            is-optional
    ag PresentationAttributes-TextElements
        # The following presentation attributes apply to 'text' elements
        a writing-mode
            is-optional
            s-type 
                restrict : string
                    enum lr-tb
                    enum rl-tb
                    enum tb-rl
                    enum lr
                    enum rl
                    enum tb
                    enum inherit
    ag PresentationAttributes-Viewports
        # The following presentation attributes apply to elements that establish viewports
        a clip
            type svg:ClipValueType
            is-optional
        a overflow
            is-optional
            s-type 
                restrict : string
                    enum visible
                    enum hidden
                    enum scroll
                    enum auto
                    enum inherit
    ag PresentationAttributes-All
        # The following represents the complete list of presentation attributes
        ag-ref svg:PresentationAttributes-Color
        ag-ref svg:PresentationAttributes-Containers
        ag-ref svg:PresentationAttributes-feFlood
        ag-ref svg:PresentationAttributes-FillStroke
        ag-ref svg:PresentationAttributes-FilterPrimitives
        ag-ref svg:PresentationAttributes-FontSpecification
        ag-ref svg:PresentationAttributes-Gradients
        ag-ref svg:PresentationAttributes-Graphics
        ag-ref svg:PresentationAttributes-Images
        ag-ref svg:PresentationAttributes-LightingEffects
        ag-ref svg:PresentationAttributes-Markers
        ag-ref svg:PresentationAttributes-TextContentElements
        ag-ref svg:PresentationAttributes-TextElements
        ag-ref svg:PresentationAttributes-Viewports
