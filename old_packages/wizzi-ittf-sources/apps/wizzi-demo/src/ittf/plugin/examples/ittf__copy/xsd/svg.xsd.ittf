        a type
            is-required
            s-type 
                restrict : string
                    enum identity
                    enum table
                    enum discrete
                    enum linear
                    enum gamma
        a tableValues
            type string
            is-optional
        a slope
            type double
            is-optional
        a intercept
            type double
            is-optional
        a amplitude
            type double
            is-optional
        a exponent
            type double
            is-optional
        a offset
            type double
            is-optional
