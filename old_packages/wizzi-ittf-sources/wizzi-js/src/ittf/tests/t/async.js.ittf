$group
	$params call, arg1|@@null, arg2|@@null, arg3|@@null, arg4|@@null, arg5|@@null, arg6|@@null

	_ ${call}
		$if arg1 != null
			@ ${arg1}
		$if arg2 != null
			@ ${arg2}
		$if arg3 != null
			@ ${arg3}
		$if arg4 != null
			@ ${arg4}
		$if arg5 != null
			@ ${arg5}
		$if arg6 != null
			@ ${arg6}

		function
			param err
			param result
			if err
				throw new Error()
			$hook
