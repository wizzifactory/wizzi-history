$group

	@ getConsoleScriptList
		function
			param options
			param callback

			function cb_list
				_ callback
					@ null
					{
						@ data
							{
								@ jsmodels jobsStore.consoleScriptList
						@ status
							_ success
								@ 'Got console script list'

			if jobsStore.consoleScriptList && !options.refresh
				return
