$group

	@ getDocumentMeta
		function
			param options
			param callback

			var documentUri
				_ paths.documentMeta
					@ options

			read_back_json( documentUri, Ittf document meta

	@ getDocument
		function
			param uri
			param callback

			var documentUri
				_ paths.pathFromUri
					@ uri
						
			read_back( documentUri, Ittf document, uri