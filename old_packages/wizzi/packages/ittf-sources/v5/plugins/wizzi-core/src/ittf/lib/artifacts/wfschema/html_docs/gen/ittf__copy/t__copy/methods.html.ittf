$group

	h5 Methods
	small
		table( 100 )
			thead
				tr
					th Signature
					th Statements
			tbody
				$foreach method in element.methods
					tr
						td 
							div ${method.name}
							$if method.comments.length > 0
								div
									$foreach comment in method.comments
										span ${comment} &nbsp;
						td
							$foreach stm in method.statements
								div ${stm.tag} ${stm.value};


