        + .splash__section
        line-height 1.5em
    
	. main-content h1
        + .splash__section h1
        font-size 33px
    
	. main-content h2
        + .splash__section h2
        font-size 28px
    
	. main-content h3
        + .splash__section h3
        font-size 23px
    
	. main-content h4
        + .splash__section h4
        font-size 19px
    
	. main-content h5
        + .splash__section h5
        font-size 16px
    
	. main-content h6
        + .splash__section h6
        font-size 13px
    
	. main-content h1
        + .main-content h2
        + .main-content h3
        + .main-content h4
        + .main-content h5
        + .main-content h6
