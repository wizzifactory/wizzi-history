$group

	$include app/options/init

	$
		app.name = 'wizzi-ui';
		app.version = '0.0.1';
		app.title = 'Wizzi ui library';
		app.description = 'UI components for the wizzi factory';
		app.keywords = 'wizzi ui react';

		app.libraryName = 'WizziUI';
		app.libraryTarget = 'var';
		
		app.useMaterialUi = true;
		app.useMonaco = true;
		app.useJsonView = true;

	$include app/options/analize
