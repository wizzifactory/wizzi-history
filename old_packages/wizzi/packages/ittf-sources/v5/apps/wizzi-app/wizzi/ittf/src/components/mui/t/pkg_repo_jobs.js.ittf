$group

	{
		&&
			+ item.toPackageSource
			< Tooltip
				@ title Edit source
				< IconButton
					@ className {classes.navLinkButton}
					@ component {Link}
					@ to {item.toPackageSource}
					< EditIcon
	{
		&&
			+ item.toPackageJobs
			< Tooltip
				@ title Execute jobs
				< IconButton
					@ className {classes.navLinkButton}
					@ component {Link}
					@ to {item.toPackageJobs}
					< LaunchIcon