$group

	$include app/options/init

	$
		app.name = 'wizzi-app';
		app.version = '0.0.1';
		app.title = 'Wizzi app';
		app.description = 'Wizzi factory app';
		app.keywords = 'wizzi ui react';
		
		app.useRedux = true;
		app.initialLoad = true;
		app.useApiMiddleware = true;
		app.apiMiddleware.server = {
			https: false,
			host: 'localhost',
			port: 3014
		};
		
		app.useCreateReactApp = true;
		app.useMaterialUi = true;
		app.rootContainer = 'AppContainer';
		app.useCssBaseline = true;
		app.useReactRouter = true;

		app.useReactLowLight = true;
		app.usePrettier = false;
		app.useJsBeautify = true;

		app.useMonaco = true;

	$include app/options/analize