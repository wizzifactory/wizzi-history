$group

	$include app/options/init

	$
		app.name = 'vue-monaco';
		app.description = 'Wizzi factory demos - vue + monaco';
	
		app.useVue = true;
		app.useVuex = false;
		app.useVueRouter = false;
		app.useVuetify = false;
		app.useFirebase = false;
		app.useAxios = false;
		app.useRequireHttp = false;
		app.useDinamicImport = false;
		app.useMonaco2 = true;

	$include app/options/analize