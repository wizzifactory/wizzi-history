$

	$include app/options/init

	app.name = 'es6-mysql';
	app.description = 'Nodejs-es6 mysql app tutorial';
	app.useBabel = false;
	app.useBabelRegister = true;
	app.useMySql = true;
	app.useGraphql = true;
	app.graphql.apolloServer = false;
	app.graphql.github = true;
	app.graphql.prisma = true;
	app.useLruCache = false;
	app.useWizzi = true;
	app.wizziPlugins = ['wizzi-js', 'wizzi-web'];

	$include app/options/analize