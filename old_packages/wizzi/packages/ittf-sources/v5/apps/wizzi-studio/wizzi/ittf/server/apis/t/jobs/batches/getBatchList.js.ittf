			param callback

			var docs
				_ paths.batchesIttfDocuments

			log 'getBatchList', docs

			var sessionData = {}
			var modelContext = {}
			var artifactContext = {}
		
			_ async.map
				@ docs
				function
					param doc
					param callback
					_ globalApi.wf.loadModelAndGenerateArtifact
