							foreach item in jsonItems
								set item.fullPath = path.join(item.cwd, item.argv0)
								set item.hash = hashEncrypt(item.fullPath)
							_ callback
								@ null
								{
									@ name doc.name
									@ items jsonItems
				f_cb_api(result)
					r_cb_success_quoted( Got job batches list )
						@ batches result
