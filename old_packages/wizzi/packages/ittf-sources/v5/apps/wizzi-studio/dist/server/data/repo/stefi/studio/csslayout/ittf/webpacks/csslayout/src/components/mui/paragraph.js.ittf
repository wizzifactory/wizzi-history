r-component( Paragraph )

	style
		@ panel
			{
				@ padding '5px 20px'
				@ margin 0
				@ font "400 16px/20px 'Open Sans', sans-serif"
				@ color "#333"
	
	render
		prop classes
		prop data
		const { text, style: styleProp } = data
			
		return
			p { text }
				@ className {classes.panel}
				@style { styleProp }
						
