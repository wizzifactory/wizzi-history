module
    kind react
    import React from 'react'
    import ReactDOM from 'react-dom'
    import logo from './logo.svg'
    import './index.css'
    const App
        => 
            div 
                @ className "App"
                img 
                    @ className "App-Logo"
                    @ src {logo}
                    @ alt "React Logo"
                h1 
                    @ className "App-Title"
                    + Hello Parcel x React
    _ ReactDOM.render
        < App 
        _ document.getElementById('root')
    # Hot Module Replacement
    # Hot Module Replacement
    if module.hot
        _ module.hot.accept
