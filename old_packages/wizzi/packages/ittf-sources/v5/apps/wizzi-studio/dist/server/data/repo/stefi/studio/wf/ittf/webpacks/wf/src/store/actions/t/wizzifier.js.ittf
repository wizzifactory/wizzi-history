						{
							@ types
								[
									@ XML_WIZZIFY_REQUEST
									@ XML_WIZZIFY_SUCCESS
									@ XML_WIZZIFY_FAILURE
							@ url '/api/wf/xmlwizzify'
							@ method 'post'
							@ payload
								{
									@ source source
	+