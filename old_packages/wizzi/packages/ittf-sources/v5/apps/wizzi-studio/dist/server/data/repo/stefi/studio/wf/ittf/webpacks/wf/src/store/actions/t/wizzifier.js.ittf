								[
									@ CSS_WIZZIFY_REQUEST
									@ CSS_WIZZIFY_SUCCESS
									@ CSS_WIZZIFY_FAILURE
							@ url '/api/wf/csswizzify'
							@ method 'post'
							@ payload
								{
									@ source source
	+
	wz-const xmlWizzify
		=>
			param source
			return
				{
					@ type XML_WIZZIFY
					@ source source
					@ checked checked || false
