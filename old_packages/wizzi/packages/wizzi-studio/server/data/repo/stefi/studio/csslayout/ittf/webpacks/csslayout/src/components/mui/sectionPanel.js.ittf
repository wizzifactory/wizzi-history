r-component( SectionPanel )

	$append imports
		import builder from './MuiBuilder'

	style

	render
		prop classes
		prop data
		const { id, style: styleProp } = data
		return
			div
				@ id { id }
				@style { styleProp }
				{
					_ data.panels.map
						=>
							param panel
							param index
							var PanelComponent = builder.create(panel)
							return
								< PanelComponent
									@ key { index }
									@ data { panel }    