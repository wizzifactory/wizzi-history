f_h_group( ${id}, ${label}, ${labelclass}, ${errorModel} )
	$params id, label, labelclass, model|@@null, value|@@null, errorModel|@@null
	input
		type text
		class form-control input-sm
		id ${id}
		name ${id}
		placeholder ${label}
		$if value
			value ${value}
		$if model
			data-ng-model ${model}
		$hook