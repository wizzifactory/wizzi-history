        meta 
            http-equiv Content-Type
            content text/html; charset=UTF-8
        meta 
            http-equiv Content-Language
            content en-us
        meta 
            http-equiv imagetoolbar
            content false
        meta 
            name MSSmartTagsPreventParsing
            content true
        meta 
            name viewport
            content width=device-width,initial-scale=1
        @title GitHub Developer | GitHub Developer Guide
        link 
            rel alternate
            type application/atom+xml
            title API Changes
            href /changes.atom
        link 
            rel stylesheet
            href https://fonts.googleapis.com/css?family=Roboto:400,300,400italic,500
        link 
            href /assets/stylesheets/application.css
            rel stylesheet
            type text/css
        js https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js
        js /assets/javascripts/application.js
    body 
        class overview
        header 
            class site-header
            . container
                id header
                a 
                    class site-header-logo mt-1
                    href /
                    img 
                        src /assets/images/github-developer-logo.svg
                        alt GitHub Developer
                nav 
                    class site-header-nav
                    a GraphQL API v4
                        class site-header-nav-item
                        href /v4/
                    a REST API v3
                        class site-header-nav-item
                        href /v3/
                    a Apps
                        class site-header-nav-item
                        href /apps/
                    a Blog
                        class site-header-nav-item
                        href /changes/
                    a Early Access
                        class site-header-nav-item
                        href /early-access/
                    a Support
                        class site-header-nav-item
                        href https://github.com/contact
                    form 
                        accept-charset UTF-8
                        action #
                        class site-header-nav-item site-header-search
                        label 
                            id search-container
                            class mb-0
                            input 
                                type text
                                id searchfield
                                class form-control
                                placeholder Search…
                                
 
                                autocomplete off
                                autocorrect off
                                autocapitalize off
                                spellcheck false
                            . cancel-search
                            ul 
                                id search-results
