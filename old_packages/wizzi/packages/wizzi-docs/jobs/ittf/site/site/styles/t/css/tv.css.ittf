$group

	. wz-ui-tv-container
		clear both
		width 100%
		overflow hidden
		border 1px solid #000
		background #000
		color #ddd
	
	. wz-ui-tv-documents
		padding 0
		margin 0 auto
		list-style-type none
		position relative

	. wz-ui-tv-documents img
		width 14px
		height 14px
		vertical-align middle

	. wz-ui-tv-document-selected
		color #f00


